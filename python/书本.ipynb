    "import turtle as t\n",
    "import math\n",
    "t.pensize(2)\n",
    "t.right(90)\n",
    "t.penup()\n",
    "t.forward(200)\n",
    "t.pendown()\n",
    "t.left(90)\n",
    "r=200\n",
    "t.circle(r)\n",
    "len = r*math.sqrt(3)\n",
    "t.left(60)\n",
    "t.forward(len)\n",
    "t.left(120)\n",
    "t.forward(len)\n",
    "t.left(120)\n",
    "t.forward(len)"
